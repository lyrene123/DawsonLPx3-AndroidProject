<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.thbs.skycons"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="25" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.thbs.skycons.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.thbs.skycons.ViewAllActivity"
            android:label="All Views" >
        </activity>
        <activity
            android:name="com.thbs.skycons.SunActivity"
            android:label="Sun" >
        </activity>
        <activity
            android:name="com.thbs.skycons.CloudSunActivity"
            android:label="Sun+Cloud" >
        </activity>
        <activity
            android:name="com.thbs.skycons.CloudActivity"
            android:label="Cloud" >
        </activity>
        <activity
            android:name="com.thbs.skycons.CloudRainActivity"
            android:label="Cloud+Rain" >
        </activity>
        <activity
            android:name="com.thbs.skycons.CloudHvRainActivity"
            android:label="Cloud+Rain" >
        </activity>
        <activity
            android:name="com.thbs.skycons.CloudSnowActivity"
            android:label="Cloud+Snow" >
        </activity>
        <activity
            android:name="com.thbs.skycons.CloudFogActivity"
            android:label="Cloud+Fog" >
        </activity>
        <activity
            android:name="com.thbs.skycons.MoonActivity"
            android:label="Moon" >
        </activity>
        <activity
            android:name="com.thbs.skycons.CloudMoonActivity"
            android:label="Cloud+Moon" >
        </activity>
        <activity
            android:name="com.thbs.skycons.WindActivity"
            android:label="Wind" >
        </activity>
        <activity
            android:name="com.thbs.skycons.ThunderActivity"
            android:label="Thunder" >
        </activity>

        <provider
            android:name="com.android.tools.ir.server.InstantRunContentProvider"
            android:authorities="com.thbs.skycons.com.android.tools.ir.server.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>