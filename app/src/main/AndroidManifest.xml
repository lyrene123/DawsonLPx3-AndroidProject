<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dawsonlpx3">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="dawsonlpx3.RegisterActivity" />
        <activity android:name="dawsonlpx3.CanceledActivity" />
        <activity android:name="dawsonlpx3.ShowCancelActivity" />
        <activity android:name="dawsonlpx3.FindTeacherActivity" />
        <activity android:name="dawsonlpx3.ChooseTeacherActivity" />
        <activity android:name="dawsonlpx3.TeacherContactActivity" />
        <activity android:name="dawsonlpx3.NotesActivity" />
        <activity android:name="dawsonlpx3.ItemNoteActivity" />
        <activity android:name="dawsonlpx3.WeatherActivity" />
        <activity android:name="dawsonlpx3.AboutActivity" />
        <activity android:name="dawsonlpx3.SettingsActivity" />
    </application>

</manifest>